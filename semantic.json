{
  "base": "semantic/",
  "paths": {
    "source": {
      "config": "src/theme.config",
      "definitions": "src/definitions/",
      "site": "src/site/",
      "themes": "src/themes/"
    },
    "output": {
      "packaged": "../src/vendor/semantic/",
      "uncompressed": "../src/vendor/semantic/components/",
      "compressed": "../src/vendor/semantic/components/",
      "themes": "../src/vendor/semantic/themes/"
    },
    "clean": "dist/"
  },
  "permission": 744,
  "autoInstall": false,
  "rtl": false,
  "components": [
