    "site",
    "button",
    "container",
    "divider",
    "flag",
    "header",
    "icon",
    "image",
    "input",
    "label",
    "list",
    "loader",
    "segment",
    "form",
    "grid",
    "menu",
    "message",
    "table",
    "card",
    "comment",
    "item",
    "statistic",
    "checkbox",
    "dimmer",
    "dropdown",
    "modal",
    "popup",
    "progress",
    "rating",
    "search",
    "shape",
    "form",
    "state",
    "visibility",
    "reveal"
  ],
  "version": "2.2.6"
}
