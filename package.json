{
  "name": "ps-react-boilerplate",
  "version": "1.0.0",
  "description": "Paquitosoft custom ReactJS starter project",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --hot --config webpack.config.dev.js",
    "test": "jest --watch --onlyChanged",
    "build": "./node_modules/.bin/webpack --config webpack.config.prod.js"
  },
  "author": "Paquitosoft",
  "license": "ISC",
  "jest": {
    "verbose": true,
    "testPathDirs": [
      "<rootDir>/spec/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/js"
    ],
    "scriptPreprocessor": "<rootDir>/jest-preprocessor",
    "setupFiles": [
      "<rootDir>/spec/support/tests-setup.js"
    ]
  },
  "dependencies": {
    "better-console": "^0.2.4",
    "exports-loader": "^0.6.3",
