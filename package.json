    "jest": "^15.1.1",
    "jest-cli": "^15.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.10.0",
    "react-addons-test-utils": "^15.3.2",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.2.1",
    "sass-loader": "^4.0.2",
