    "bundle-loader": "^0.5.4",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "es6-promise": "^3.3.1",
    "eslint": "^3.5.0",
    "eslint-plugin-react": "^6.2.2",
    "file-loader": "^0.9.0",
