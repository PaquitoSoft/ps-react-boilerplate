    "nprogress": "^0.2.0",
    "object-assign": "^4.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "require-dot-file": "^0.4.0",
    "semantic-ui": "^2.2.6",
    "whatwg-fetch": "^1.0.0"
